{{!< default}}

{{#post}}
<script>
  window.__STATE_FROM_SERVER__ = {
    posts: {
      summaries: {
        status: "empty"
      },
      bySlug: {
        "{{slug}}": {
          data: {
            slug: "{{slug}}",
            title: "{{title}}",
            meta_description: "{{meta_description}}",
            meta_title: "{{meta_title}}",
            published_at: "{{publushed_at}}",
            image: "{{image}}",
            featured: "{{featured}}"
          },
          status: "success"
        }
      }
    }
  };
</script>
<script id='post-html-template' data-post-slug='{{slug}}' type='text/template'>
  {{{html}}}
</script>
{{/post}}
